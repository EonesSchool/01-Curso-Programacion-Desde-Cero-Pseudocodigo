Algoritmo numeroMayor100
	
	// Definir Variables
	Definir numeroUsuario Como Real;
	
	// Inicialización
	Escribir "Ingresa Un Valor Numérico: ";
	Leer numeroUsuario;
	
	// Condicional Simple
	Si numeroUsuario > 100 Entonces
		Escribir "El Número Ingresado Es Mayor A 100";
		Escribir "El Número Ingresado Es: ", numeroUsuario;
	FinSi
	
FinAlgoritmo
