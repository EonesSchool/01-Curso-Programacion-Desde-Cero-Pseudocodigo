Algoritmo edadNacimiento
	
	// Definir Variables O Constantes
	Definir VALOR_UNO, VALOR_DOS, edad Como Entero;
	
	// Inicialización
	VALOR_UNO = 2021;
	VALOR_DOS = 1993;
	
	// Proceso  Aritmetico
	edad = VALOR_UNO - VALOR_DOS;
	
	// Mostrar Información
	Escribir "Mi Edad Es De ", edad, " Años.";
	

FinAlgoritmo
