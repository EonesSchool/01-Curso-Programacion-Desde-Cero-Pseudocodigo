Algoritmo conversionDinamica
	
	// Definir Variables O Constantes
	Definir GRADOS_C, GRADOS_F Como Real;
	
	// Inicialización De Constantes Y Variables
	Escribir "Ingresar Grados Centigrados: ";
	Leer GRADOS_C;
	
	GRADOS_F = (GRADOS_C * 1.8) + 32;
	
	// Mostrar Información
	Escribir "";
	Escribir "Grados Centigrados Iniciales: ", GRADOS_C;
	Escribir "Conversión De Centigrados A Fahrenheit: ", GRADOS_F;

FinAlgoritmo
