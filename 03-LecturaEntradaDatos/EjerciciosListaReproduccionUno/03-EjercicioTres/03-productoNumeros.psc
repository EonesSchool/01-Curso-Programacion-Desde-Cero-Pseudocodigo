Algoritmo productoNumeros
	
	// Definir Constantes O Variables
	Definir VALOR_UNO Como Real;
	Definir VALOR_DOS Como Real;
	Definir producto como Real;
	
	// Inicialización De Constantes O Variables
	VALOR_UNO = 3;
	VALOR_DOS = 5;
	
	// Proceso Aritmético
	producto = VALOR_UNO * VALOR_DOS;
	
	// Mostrar información
	Escribir "El Producto De Los Dos Números Es: ", producto;
	
FinAlgoritmo
