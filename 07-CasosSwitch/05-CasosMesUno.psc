Algoritmo casosOswitchCinco
	
	// Definir Variables O Constantes
	Definir mes Como Cadena;
	
	// Variable A Evaluar Del Tipo De Dato Cadena
	Escribir "Ingresa Un Mes Del Año: ";
	Leer mes;
	
	Segun mes Hacer
		"diciembre":
			Escribir "Felices Fiesta Navideñas";
		"DICIEMBRE":
			Escribir "Felices Fiesta Navideñas";
		De Otro Modo:
			Escribir "Este No Se Celebra Navidad.";
	FinSegun
	
FinAlgoritmo
